	<queryString>
		<![CDATA[SELECT
	p.patient_number,
	p.name,
	pc.cost,
	pc.checkup_date
FROM
	patient_checkups pc
	inner join specialize_doctors sd ON pc.specialize_doctor_id = sd.id 
	inner join laboratorium_divisions ld ON pc.laboratorium_division_id = ld.id
	inner join patients p on pc.patient_id = p.id]]>
